package InterfacePractice;

import javax.swing.JOptionPane;

public class Tester{

	public static void main(String[] args) {
		new Tester();
	}
	
	public Tester(){
		String first = JOptionPane.showInputDialog("Put a number");
		String second = JOptionPane.showInputDialog("Input another number");
		System.out.print(betterString(first, second, (s1, s2) -> {
			
		}));
	}

	public String betterString(String s1, String s2, Sorter s){
		if(s.sort(s1, s2)){
			return s1;
		}else{
			return s2;
		}
	}
	
}
