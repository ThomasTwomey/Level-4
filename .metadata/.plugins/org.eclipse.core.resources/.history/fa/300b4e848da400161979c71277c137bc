package ThreadPractice;

public class PrimeNumbers {
	
	public static void main(String[] args) {
		int[] primes = new int[1000];
		
		new Thread(() -> {
			for(int i = 1; i < 1000; i++){
				for(int j = 2; j < 10; j++){
					if(i % j == 0){
						System.out.println(i);
					}
				}
			}
		}).start();
	}
	
	boolean isPrime(int num){
		for(int i = 2; i <= num; i++){
			if(num % i == 0){
				return false;
			}
		}
		return true;
	}

}
