package IntroToSanner;

import java.util.ArrayList;
import java.util.Scanner;

public class IntroToScanner {
	
	Scanner keyboard = new Scanner(System.in);

	public static void main(String[] args) {
		new IntroToScanner();
	}
	
	IntroToScanner(){
		ArrayList<String> userWords = new ArrayList<String>();
		String input = "";
		
		while(!input.equals("0")){
			input = keyboard.next();
			if(input.equals("0"))
				break;
			userWords.add(input);
		}
	}
	
	public ArrayList<String> sortWords(ArrayList<String> userWords){
		ArrayList<String> sortedWords = new ArrayList<String>();
		for(String a: userWords){
			for(String b: userWords){
				
			}
		}
		return sortedWords;
	}

}
