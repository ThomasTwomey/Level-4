package Polymorphism;

import java.awt.Color;
import java.awt.Graphics;

public class Polymorph {
    private int x;
    private int y;
    
    Polymorph(int x, int y){
   	 this.x = x;
   	 this.y = y;
    }
    
    public void update(){
   	 
    }
    
    public void draw(Graphics g){
   	 g.setColor(Color.BLUE);
   	 g.fillRect(x, y, 10, 10);
    }
}
