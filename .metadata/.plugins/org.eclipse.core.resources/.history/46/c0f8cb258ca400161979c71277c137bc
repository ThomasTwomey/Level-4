package ThreadPractice;

public class ThreadPractice {
	public static void main(String[] args) {
		new Thread(() -> {
			for(int i = 0; i < 10; i++){
				System.out.println(i + " loops");
			}
		}).start();
		new Thread(() -> {
			for(int i = 0; i < 10; i++){
				System.out.println(i + " loops 2");
			}
		}).start();
	System.out.println("End of class");
	}
}
