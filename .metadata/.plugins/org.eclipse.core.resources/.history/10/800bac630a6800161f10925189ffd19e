package Polymorphism;

import java.util.Random;

public class MovingMorph extends Polymorph{
	private int counter;
	
	MovingMorph(int x, int y, int width, int height) {
		super(x, y, width, height);
		
	}
	
	@Override
	public void update(){
		int xPos = getX();
		int yPos = getY();
		setX(counter);
		setY(counter);
	}
	
}
