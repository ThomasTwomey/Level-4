package IntroToSanner;

import java.util.ArrayList;
import java.util.Scanner;

public class IntroToScanner {
	
	Scanner keyboard = new Scanner(System.in);

	public static void main(String[] args) {
		new IntroToScanner();
	}
	
	IntroToScanner(){
		ArrayList<String> userWords = new ArrayList<String>();
		String input = "";
		boolean tof = true;
		
		while(tof == true){
			input = keyboard.next();
			if(input.equals("0")){
				tof = false;
			}
			userWords.add(input);
		}
		
		userWords = sortWords(userWords);
		System.out.println(userWords);
	}
	
	public ArrayList<String> sortWords(ArrayList<String> userWords){
		boolean tof = true;
		String temp;
		
		for(int i = 0; i < userWords.size() - 1; i++){
			if(userWords.get(i).compareTo(userWords.get(i+1)) > 0){
				temp = userWords.get(i);
				userWords.set(i, userWords.get(i+1));
				userWords.set(i+1, temp);
			}

		}
		return userWords;
	}

}